                "rules": [
                    {
                        "description": "fn+# will set input_source aka change language",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "0",
                                    "modifiers": { "mandatory": ["fn"] }
                                },
                                "to": [{ "shell_command": "/usr/local/bin/macism com.apple.keylayout.USExtended" }],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "9",
                                    "modifiers": { "mandatory": ["fn"] }
                                },
                                "to": [{ "shell_command": "/usr/local/bin/macism com.apple.inputmethod.Korean.2SetKorean" }],
                                "type": "basic"
                            },
